[
    {
        "//": "from sender",
        
        "type": "msg",
        "data": {
            "from": "${from}",
            "to": "${to}",
            "content": "${message}",
            "time": "${datetime}"
        },
        "attach": "${sender-msg-stamp}"
    },

    {
        "//": "reply to the sender",

        "data": {
            "reply": "ok",
            "stamp": "${sender-msg-stamp}"
        },
        "attach": "${empty}"
    }
]